            <termEntry>
                <langSet xml:lang="en"><tig><term>(Jerry) "Hello!"</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>(Jerry) "Hallo!"</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>(Forged ID) Try to trick the Elite.</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>(Gefälschte Ausweise) Du versuchst das Elite-Schiff auszutricksen.</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>Continue...</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>Fortfahren...</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>(Adv. Engines) Try to escape the Elite.</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>(Erw. Triebwerke) Du versuchst dem Elite-Schiff zu entkommen.</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>Do something onboard the ship.</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>Tue etwas an Bord des Schiffs.</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>Do nothing.</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>Nichts tun.</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>Attack, we can steal their Mine Launcher tech!</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>Angriff, wir können ihre Minenwerfer Technologie stehlen!</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>You've taken the risky choice of fighting the ship. Hopefully, you can protect yourself from their salvo long enough.</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>Du hast das Risiko auf dich genommen gegen das Schiff zu kämpfen. Hoffentlich kannst du dich lange genug vor ihren Salven schützen.</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>Avoid the Trapper.</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>Gehe dem Trapper aus dem Weg.</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>Messing with a Trapper is a bad idea.</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>Mit dem Trapper Streit anzufangen ist eine schlechte Idee.</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>"A great purchase. May they serve you well, captain..."</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>"Ein großartiger Kauf. Mögen sie dir gute Dienste leisten, Kapitän..."</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>[Federation Database] What is Tiiikaka?</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>[Förderation Datenbank] Was ist Tiiikaka?</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>Decline.</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>Ablehnen.</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>Board the ship.</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>Betrete das Schiff.</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>Ignore it.</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>Ignoriere es.</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>Do not intervene.</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>Mische dich nicht ein.</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>Accept their offer.</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>Nimm ihr Angebot an.</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>Charge FTL drive.</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>FTL-Antrieb aufladen.</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>Attack!</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>Angriff!</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>Ignore the station.</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>Ignoriere die Station.</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>There likely isn't anything good to come out of exploring this place. Let's move on.</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>Diesen Ort zu erkunden wird nichts gutes mit sich bringen. Lass uns weiter gehen.</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>Leave.</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>Gehe wieder.</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>Agree.</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>Zustimmen.</term></tig></langSet>
            </termEntry>
            <termEntry>
                <langSet xml:lang="en"><tig><term>Human</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>Mensch</term></tig></langSet>
            </termEntry>
            <termEntry id="1">
                <langSet xml:lang="en"><tig><term>Human</term></tig></langSet>
                <langSet xml:lang="de"><tig><term>Mensch</term></tig></langSet>
            </termEntry>
            <termEntry>
